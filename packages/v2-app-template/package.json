{
  "name": "v2-app-template",
  "version": "0.0.0",
  "private": true,
  "description": "Small description for v2-app-template goes here",
  "repository": "",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "vite build",
    "format": "prettier . --cache --write",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\" --prefixColors auto",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"pnpm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" --prefixColors auto && pnpm format",
    "lint:format": "prettier . --cache --check",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "vite",
    "test": "vite build --mode development && ember test --path dist"
  },
  "exports": {
    "./package.json": "./package.json",
    "./tests/*": "./tests/*",
    "./*": "./app/*"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/runtime": "^7.28.4",
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@babel/eslint-parser": "^7.28.4",
    "@ember/optional-features": "^2.2.0",
    "@ember/string": "^4.0.1",
    "@ember/test-helpers": "^5.3.0",
    "@ember/test-waiters": "^4.1.1",
    "@embroider/macros": "^1.19.1",
    "@embroider/core": "^4.2.4",
    "@embroider/vite": "^1.3.2",
    "@embroider/compat": "^4.1.7",
    "@embroider/router": "^3.0.4",
    "@embroider/config-meta-loader": "^1.0.0",
    "@embroider/legacy-inspector-support": "^0.1.3",
    "@eslint/js": "^9.37.0",
    "@glimmer/component": "^2.0.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@warp-drive/ember": "~5.6.0",
    "babel-plugin-ember-template-compilation": "^2.4.1",
    "concurrently": "^9.2.1",
    "decorator-transforms": "^2.3.0",
    "ember-cli": "~6.8.0",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-deprecation-workflow": "^3.4.0",
    "ember-data": "~5.6.0",
    "ember-load-initializers": "^3.0.1",
    "ember-modifier": "^4.2.2",
    "ember-page-title": "^9.0.3",
    "ember-qunit": "^9.0.4",
    "ember-resolver": "^13.1.1",
    "ember-source": "~6.8.0",
    "ember-template-lint": "^7.9.3",
    "ember-welcome-page": "^7.0.2",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-ember": "^12.7.4",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-qunit": "^8.2.5",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "prettier-plugin-ember-template-tag": "^2.1.0",
    "qunit": "^2.24.2",
    "qunit-dom": "^3.5.0",
    "stylelint": "^16.25.0",
    "stylelint-config-standard": "^38.0.0",
    "testem": "^3.16.0",
    "tracked-built-ins": "^4.0.0",
    "vite": "^7.1.9"
  },
  "engines": {
    "node": ">= 20"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "__comment__": "this is needed for our tests until we fix @embroider/addon-shim to not throw under embroider",
    "version": 2
  }
}
